{
  "name": "filter-syn",
  "displayName": "Filter-Syn",
  "description": "AI-powered code filtering and analysis",
  "version": "1.0.33",
  "publisher": "PriyadarshanKarnati",
  "icon": "images/icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/darshan276141/filter-syntax.git"
  },
  "galleryBanner.color": {
    "color": "#21214c",
    "theme": "dark"
  },
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Programming Languages",
    "Linters"
  ],
  "activationEvents": [
    "onLanguage:javascript",
    "onLanguage:typescript",
    "onCommand:filter-syn.removeUnusedCode",
    "onCommand:filter-syn.undoLastFilter"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "filter-syn.removeUnusedCode",
        "title": "Filter-Syn: Remove Unused Code",
        "category": "Filter-Syn"
      },
      {
        "command": "filter-syn.undoLastFilter",
        "title": "Filter-Syn: Undo Last Filter",
        "category": "Filter-Syn"
      }
    ],
    "configuration": {
      "title": "Filter-Syn",
      "properties": {
        "filter-syn.pythonPath": {
          "type": "string",
          "default": "python",
          "description": "The absolute path to the Python executable to use for analysis."
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile:production",
    "compile": "npm run build && npm run copy-assets",
    "compile:production": "npm run build:production && npm run copy-assets",
    "build": "node ./esbuild.js",
    "build:production": "node ./esbuild.js --production",
    "copy-assets": "copyfiles -u 1 \"ai_backend/**/*.py\" out && copyfiles -u 1 \"scripts/**/*.py\" out",
    "watch": "node ./esbuild.js --watch"
  },
  "devDependencies": {
    "@types/vscode": "^1.80.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.5.6",
    "@vscode/test-electron": "^2.3.4",
    "esbuild": "^0.19.2",
    "copyfiles": "^2.4.1",
    "typescript": "^5.2.2",
    "@babel/parser": "^7.28.3",
    "node-fetch": "^3.3.2"
  }
}