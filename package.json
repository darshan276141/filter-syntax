{
  "name": "filter-syn",
  "displayName": "Filter-Syn",
  "description": "AI-powered code filtering and analysis",
  "version": "1.0.0",
  "publisher": "Priyadarshan",
  "icon": "images/icon.png",
  "galleryBanner.color": {
    "color": "#21214c",
    "theme": "dark"
  },
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Programming Languages",
    "Linters"
  ],
  "activationEvents": [
    "onLanguage:javascript",
    "onLanguage:typescript",
    "onCommand:filter-syn.removeUnusedCode",
    "onCommand:filter-syn.showUnusedCodePanel",
    "onCommand:filter-syn.undoLastFilter",
    "onCommand:filter-syn.aiFilterSuggestion"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "filter-syn.removeUnusedCode",
        "title": "Filter-Syn: Remove Unused Code",
        "category": "Filter-Syn"
      },
      {
        "command": "filter-syn.undoLastFilter",
        "title": "Filter-Syn: Undo Last Filter",
        "category": "Filter-Syn"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.5.6",
    "typescript": "^5.2.2",
    "vscode": "^1.1.37"
  },
  "dependencies": {
    "@babel/generator": "^7.28.3",
    "@babel/parser": "^7.28.3",
    "@babel/traverse": "^7.28.3",
    "abort-controller": "^3.0.0",
    "node-fetch": "^3.3.2",
    "web-tree-sitter": "^0.25.8"
  }
}
